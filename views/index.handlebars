{{#if projects}}
    {{#each projects}}
       <div class="column">
       <img class="thumbnail" src="./public/images/portfolio{{image}}">
       <h5 class="text-center">{{title}}</h5>
       <p class="text-center show"><a href="/details/{{id}}">
       Show More</a></p>
       </div>
    {{/each}}
{{else}}
    <p>No Projects</p>
{{/if}}